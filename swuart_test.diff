 #include "arduino.h"
 #include "serial.h"
 
+#include "SoftwareSerial.h"
+SoftwareSerial test = SoftwareSerial(63, 6, false);
+
 static __INLINE uint32_t SysTick_Config(uint32_t ticks) {
   if (ticks > SysTick_LOAD_RELOAD_Msk)
     return (1); /* Reload value impossible */
